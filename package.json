{
  "name": "angular-workshop-performance-demo",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:stats": "ng build --named-chunks=false --output-hashing=none --stats-json",
